{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "publish": {
      "dependsOn": [
        "^publish"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "lint": {}
  }
}